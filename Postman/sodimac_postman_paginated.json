{
    "info": {
        "_postman_id": "sodimac-auth-collection",
        "name": "Sodimac Auth API",
        "description": "Postman collection for testing Sodimac authentication API endpoints.",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Auth",
            "item": [
                {
                    "name": "Login",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Accept",
                                "value": "application/json"
                            },
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "url": {
                            "raw": "{{baseURL}}/api/Auth/login",
                            "host": [
                                "{{baseURL}}"
                            ],
                            "path": [
                                "api",
                                "Auth",
                                "login"
                            ]
                        },
                        "body": {
                            "mode": "raw",
                            "raw": "{\"email\": \"admin@sodimac.com\", \"contraseña\": \"123456\"}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "description": "Inicia sesión de usuario con email y contraseña."
                    },
                    "response": []
                }
            ]
        }
    ],
    "variable": [
        {
            "key": "baseURL",
            "value": "https://localhost:7036",
            "description": "Base URL of the API for testing"
        }
    ]
}